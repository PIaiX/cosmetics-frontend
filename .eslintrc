{
    "extends": [
        "react-app",
        "react-app/jest",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "plugins": ["react", "prettier"],
    "rules": {
        "no-unused-vars": "off",
        "no-useless-catch": "off",
        "react/prop-types": "off",
        "no-irregular-whitespace": "off"
    }
}
